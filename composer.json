{
    "name": "payment-ag/payment-module",
    "description": "Payment Module",
    "type": "oxideshop-module",
    "version": "1.0.1",
    "keywords": [
        "oxid",
        "modules",
        "paymentAG"
    ],
    "license": [
        "GPL-3.0"
    ],
    "extra": {
        "oxideshop": {
            "blacklist-filter": [
                "documentation/**/*.*"
            ],
            "target-directory": "paymentag/paymentmodule"
        }
    },
    "require": {
        "php": ">=5.6",
        "lib-curl": ">=7.26.0",
        "lib-openssl": ">=1.0.1",
        "ext-curl": "*",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "PaymentAG\\PaymentModule\\": "src/"
        }
    }
}
